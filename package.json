{
    "name": "ng2-scroll-to",
    "version": "1.0.7",
    "description": "angular 2 library to animate scrolling.",
    "scripts": {
        "build": "ngc -p tsconfig.json",
        "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
        "test": "tsc && karma start",
        "prepublish": "npm run clean && npm run build",
        "cleanRoot": "rm -f  ./*js ./*.js.map ./*.d.ts ./*.metadata.json 2> /dev/null",
        "cleanSrc": "rm -f ./src/*js ./src/*.js.map ./src/*.d.ts ./src/*.metadata.json 2> /dev/null",
        "clean": "npm run cleanRoot && npm run cleanSrc",
        "tsc": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drusso85/ng2-scroll-to"
    },
    "author": "drusso85",
    "keywords": [
        "smooth",
        "scroll",
        "smoothScroll",
        "angular 2",
        "angular2",
        "scrollTo",
        "scrollTop"
    ],
    "license": "MIT",
    "main": "./index.js",
    "dependencies": {},
    "peerDependencies": {
        "@angular/core": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/router": "^5.2.0",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/core": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-server": "^5.2.0",
        "codelyzer": "^4.0.1",
        "rxjs": "^5.5.6",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3",
        "zone.js": "^0.8.19",
        "@types/jasmine": "~2.8.3",
        "@types/selenium-webdriver": "^2.53.39"
    },
    "engines": {
        "node": ">=0.8.0"
    }
}